#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_011034A0 .scope module, "Counter_tb" "Counter_tb" 2 3;
 .timescale 0 0;
v011046E0_0 .var "clock", 0 0;
v01104738_0 .net "data_out", 7 0, v01104688_0; 1 drivers
S_01103528 .scope module, "dut" "Counter" 2 8, 3 1, S_011034A0;
 .timescale 0 0;
v01108F28_0 .net "clock", 0 0, v011046E0_0; 1 drivers
v01104630_0 .var/i "counter", 31 0;
v01104688_0 .var "data_out", 7 0;
E_01108D08 .event posedge, v01108F28_0;
    .scope S_01103528;
T_0 ;
    %set/v v01104630_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_01103528;
T_1 ;
    %wait E_01108D08;
    %load/v 8, v01104630_0, 32;
    %cmpi/u 8, 10, 32;
    %jmp/0xz  T_1.0, 4;
    %set/v v01104630_0, 0, 32;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v01104630_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v01104630_0, 8, 32;
T_1.1 ;
    %load/v 8, v01104630_0, 32;
    %set/v v01104688_0, 8, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_011034A0;
T_2 ;
    %vpi_call 2 14 "$dumpfile", "dump.vcd";
    %vpi_call 2 15 "$dumpvars", 1'sb0, S_011034A0;
    %set/v v011046E0_0, 0, 1;
    %delay 40, 0;
    %vpi_call 2 19 "$finish";
    %end;
    .thread T_2;
    .scope S_011034A0;
T_3 ;
    %delay 1, 0;
    %load/v 8, v011046E0_0, 1;
    %inv 8, 1;
    %set/v v011046E0_0, 8, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Counter_tb.v";
    "./Counter.v";
