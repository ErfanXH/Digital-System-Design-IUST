#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_011D7C90 .scope module, "TrafficLight_tb" "TrafficLight_tb" 2 3;
 .timescale 0 0;
v011D3558_0 .var "Clock", 0 0;
v011DBA30_0 .net "Green", 0 0, v011DBE50_0; 1 drivers
v011DBA88_0 .net "Red", 0 0, v011DBEA8_0; 1 drivers
v011DBAE0_0 .var "Reset", 0 0;
v011DBB38_0 .net "Yellow", 0 0, v011D3500_0; 1 drivers
S_011D89D8 .scope module, "TrafficLight0" "TrafficLight" 2 11, 3 1, S_011D7C90;
 .timescale 0 0;
P_011D9C2C .param/l "GreenTime" 3 11, +C4<011001>;
P_011D9C40 .param/l "RedTime" 3 9, +C4<010100>;
P_011D9C54 .param/l "RedYellowTime" 3 10, +C4<0101>;
P_011D9C68 .param/l "YellowTime" 3 12, +C4<0101>;
v011D3940_0 .net "Clock", 0 0, v011D3558_0; 1 drivers
v011D39C8_0 .var/i "Counter", 31 0;
v011DBE50_0 .var "Green", 0 0;
v011DBEA8_0 .var "Red", 0 0;
v011DBF00_0 .net "Reset", 0 0, v011DBAE0_0; 1 drivers
v011D34A8_0 .var "State", 1 0;
v011D3500_0 .var "Yellow", 0 0;
E_011DE450/0 .event negedge, v011D3940_0;
E_011DE450/1 .event posedge, v011DBF00_0, v011D3940_0;
E_011DE450 .event/or E_011DE450/0, E_011DE450/1;
    .scope S_011D89D8;
T_0 ;
    %set/v v011DBEA8_0, 0, 1;
    %set/v v011D3500_0, 0, 1;
    %set/v v011DBE50_0, 0, 1;
    %set/v v011D34A8_0, 0, 2;
    %set/v v011D39C8_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_011D89D8;
T_1 ;
    %wait E_011DE450;
    %load/v 8, v011DBF00_0, 1;
    %jmp/0xz  T_1.0, 8;
    %set/v v011D34A8_0, 0, 2;
    %set/v v011DBEA8_0, 0, 1;
    %set/v v011D3500_0, 0, 1;
    %set/v v011DBE50_0, 0, 1;
    %set/v v011D39C8_0, 0, 32;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v011D34A8_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.2, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.3, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.4, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_1.5, 6;
    %jmp T_1.6;
T_1.2 ;
    %set/v v011DBEA8_0, 1, 1;
    %set/v v011D3500_0, 0, 1;
    %set/v v011DBE50_0, 0, 1;
    %load/v 8, v011D39C8_0, 32;
    %mov 40, 39, 1;
    %addi 8, 5, 33;
    %set/v v011D39C8_0, 8, 32;
    %load/v 8, v011D39C8_0, 32;
    %cmpi/u 8, 20, 32;
    %jmp/0xz  T_1.7, 4;
    %movi 8, 1, 2;
    %set/v v011D34A8_0, 8, 2;
T_1.7 ;
    %jmp T_1.6;
T_1.3 ;
    %set/v v011DBEA8_0, 1, 1;
    %set/v v011D3500_0, 1, 1;
    %set/v v011DBE50_0, 0, 1;
    %load/v 8, v011D39C8_0, 32;
    %mov 40, 39, 1;
    %addi 8, 5, 33;
    %set/v v011D39C8_0, 8, 32;
    %load/v 8, v011D39C8_0, 32;
    %cmpi/u 8, 25, 32;
    %jmp/0xz  T_1.9, 4;
    %movi 8, 2, 2;
    %set/v v011D34A8_0, 8, 2;
T_1.9 ;
    %jmp T_1.6;
T_1.4 ;
    %set/v v011DBEA8_0, 0, 1;
    %set/v v011D3500_0, 0, 1;
    %set/v v011DBE50_0, 1, 1;
    %load/v 8, v011D39C8_0, 32;
    %mov 40, 39, 1;
    %addi 8, 5, 33;
    %set/v v011D39C8_0, 8, 32;
    %load/v 8, v011D39C8_0, 32;
    %cmpi/u 8, 50, 32;
    %jmp/0xz  T_1.11, 4;
    %set/v v011D34A8_0, 1, 2;
T_1.11 ;
    %jmp T_1.6;
T_1.5 ;
    %set/v v011DBEA8_0, 0, 1;
    %set/v v011D3500_0, 1, 1;
    %set/v v011DBE50_0, 0, 1;
    %load/v 8, v011D39C8_0, 32;
    %mov 40, 39, 1;
    %addi 8, 5, 33;
    %set/v v011D39C8_0, 8, 32;
    %load/v 8, v011D39C8_0, 32;
    %cmpi/u 8, 55, 32;
    %jmp/0xz  T_1.13, 4;
    %set/v v011D34A8_0, 0, 2;
    %set/v v011D39C8_0, 0, 32;
T_1.13 ;
    %jmp T_1.6;
T_1.6 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_011D7C90;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "dump.vcd";
    %vpi_call 2 21 "$dumpvars", 1'sb0, S_011D7C90;
    %set/v v011D3558_0, 0, 1;
    %set/v v011DBAE0_0, 0, 1;
    %delay 10, 0;
    %set/v v011DBAE0_0, 1, 1;
    %delay 10, 0;
    %set/v v011DBAE0_0, 0, 1;
    %delay 105, 0;
    %set/v v011DBAE0_0, 1, 1;
    %delay 10, 0;
    %set/v v011DBAE0_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 28 "$finish";
    %end;
    .thread T_2;
    .scope S_011D7C90;
T_3 ;
    %delay 5, 0;
    %load/v 8, v011D3558_0, 1;
    %inv 8, 1;
    %set/v v011D3558_0, 8, 1;
    %jmp T_3;
    .thread T_3;
